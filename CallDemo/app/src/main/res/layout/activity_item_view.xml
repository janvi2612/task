<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="wrap_content"
xmlns:app="http://schemas.android.com/apk/res-auto"
android:id="@+id/rowlayout">


   <TextView
       android:id="@+id/contno"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Contact no"
       android:textSize="24sp"
       app:layout_constraintBottom_toBottomOf="parent"
       app:layout_constraintTop_toTopOf="parent"
       app:layout_constraintVertical_bias="1.0"
       tools:ignore="MissingConstraints"
       tools:layout_editor_absoluteX="16dp" />

   <ImageView
        android:id="@+id/callmade"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_baseline_call_made_24"
        tools:layout_editor_absoluteX="338dp"
        tools:layout_editor_absoluteY="4dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/callrecieved"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_baseline_call_received_24"
        tools:layout_editor_absoluteX="371dp"
        tools:layout_editor_absoluteY="4dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>